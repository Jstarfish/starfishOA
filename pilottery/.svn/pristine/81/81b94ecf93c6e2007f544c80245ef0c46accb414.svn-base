package cls.pilottery.oms.issue.model;

import cls.pilottery.common.model.BaseEntity;
/**
 * 游戏只读参数(GP_STATIC)
 * 
 * @author Woo
 */
public class GameParameterStatic extends BaseEntity{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 8875130253338158269L;

	private Short gameCode; 	  		//GAME_CODE					游戏编码
	private Long drawMode;	  			//DRAW_MODE	 				开奖模式（1=快开、2=内部算奖、3=外部算奖）
	private Long singleBetAmount;		//SINGLEBET_AMOUNT			单注投注金额（单位分）
	private Long singleTicketMaxIssues;	//SINGLETICKET_MAX_ISSUES	多期销售期数限制（1-20）
	private Long limitBigPrize;			//LIMIT_BIG_PRIZE			大奖金额（单位分）
	private Long limitPayment;			//LIMIT_PAYMENT				游戏兑奖限额（单位分）
	private Long limitPayment2;			//LIMIT_PAYMENT2			“二级区域”兑奖金额上限（单位分）
	private Long limitCancel2; 			//LIMIT_CANCEL2				““二级区域”退票金额上限（单位分） 
	private Short abandonRewardCollect;	//ABANDON_REWARD_COLLECT	弃奖去处（1=奖池、2=调节基金、3=公益金）
	
	public Short getGameCode() {
		return gameCode;
	}
	public void setGameCode(Short gameCode) {
		this.gameCode = gameCode;
	}
	public Long getDrawMode() {
		return drawMode;
	}
	public void setDrawMode(Long drawMode) {
		this.drawMode = drawMode;
	}
	public Long getSingleBetAmount() {
		return singleBetAmount;
	}
	public void setSingleBetAmount(Long singleBetAmount) {
		this.singleBetAmount = singleBetAmount;
	}
	public Long getSingleTicketMaxIssues() {
		return singleTicketMaxIssues;
	}
	public void setSingleTicketMaxIssues(Long singleTicketMaxIssues) {
		this.singleTicketMaxIssues = singleTicketMaxIssues;
	}
	public Long getLimitBigPrize() {
		return limitBigPrize;
	}
	public void setLimitBigPrize(Long limitBigPrize) {
		this.limitBigPrize = limitBigPrize;
	}
	public Long getLimitPayment() {
		return limitPayment;
	}
	public void setLimitPayment(Long limitPayment) {
		this.limitPayment = limitPayment;
	}
	public Long getLimitPayment2() {
		return limitPayment2;
	}
	public void setLimitPayment2(Long limitPayment2) {
		this.limitPayment2 = limitPayment2;
	}
	public Short getAbandonRewardCollect() {
		return abandonRewardCollect;
	}
	public void setAbandonRewardCollect(Short abandonRewardCollect) {
		this.abandonRewardCollect = abandonRewardCollect;
	}
	public Long getLimitCancel2() {
		return limitCancel2;
	}
	public void setLimitCancel2(Long limitCancel2) {
		this.limitCancel2 = limitCancel2;
	}

}
