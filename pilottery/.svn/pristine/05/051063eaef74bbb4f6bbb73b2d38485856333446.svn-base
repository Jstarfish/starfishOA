package cls.pilottery.oms.common.msg;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

public class UpdateGameAuth5011Req implements Serializable {

	private static final long serialVersionUID = -1543934453728552026L;
	private Integer ctrlLevel;
	private String ctrlCode;
	private Integer gameCount;
	private List<AreaGameCtrl> ctrls = new ArrayList<AreaGameCtrl>();;
	
	public Integer getCtrlLevel() {
		return ctrlLevel;
	}
	public void setCtrlLevel(Integer ctrlLevel) {
		this.ctrlLevel = ctrlLevel;
	}
	public String getCtrlCode() {
		return ctrlCode;
	}
	public void setCtrlCode(String ctrlCode) {
		this.ctrlCode = ctrlCode;
	}
	public Integer getGameCount() {
		return gameCount;
	}
	public void setGameCount(Integer gameCount) {
		this.gameCount = gameCount;
	}
	public List<AreaGameCtrl> getCtrls() {
		return ctrls;
	}
	public void setCtrls(List<AreaGameCtrl> ctrls) {
		this.ctrls = ctrls;
	}
}