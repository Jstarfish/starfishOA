package cls.pilottery.oms.issue.model;

import cls.pilottery.common.model.BaseEntity;
/**
 * 游戏普通参数视图(v_gp_normal_rule)
 * 
 * @author Woo
 */
public class GameParameterNormalView extends BaseEntity{

	/**
	 * 
	 */
	private static final long serialVersionUID = 7092222557865131706L;

	private Short gameCode; 	//GAME_CODE
	private Short drawMode; 	//DRAW_MODE
	private Long singleBetAmount;			//SINGLEBET_AMOUNT
	private Short singleTicketMaxIssues;	//SINGLETICKET_MAX_ISSUES
	private Long singleLineMaxAmount;		//SINGLELINE_MAX_AMOUNT
	private Long singleTicketMaxLine;		//SINGLETICKET_MAX_LINE
	private Long singleTicketMaxAmount;		//SINGLETICKET_MAX_AMOUNT
	private Short abandonRewardCollect;		//ABANDON_REWARD_COLLECT
	
	public Short getGameCode() {
		return gameCode;
	}
	public void setGameCode(Short gameCode) {
		this.gameCode = gameCode;
	}
	public Short getDrawMode() {
		return drawMode;
	}
	public void setDrawMode(Short drawMode) {
		this.drawMode = drawMode;
	}
	public Long getSingleBetAmount() {
		return singleBetAmount;
	}
	public void setSingleBetAmount(Long singleBetAmount) {
		this.singleBetAmount = singleBetAmount;
	}
	public Short getSingleTicketMaxIssues() {
		return singleTicketMaxIssues;
	}
	public void setSingleTicketMaxIssues(Short singleTicketMaxIssues) {
		this.singleTicketMaxIssues = singleTicketMaxIssues;
	}
	public Long getSingleLineMaxAmount() {
		return singleLineMaxAmount;
	}
	public void setSingleLineMaxAmount(Long singleLineMaxAmount) {
		this.singleLineMaxAmount = singleLineMaxAmount;
	}
	public Long getSingleTicketMaxLine() {
		return singleTicketMaxLine;
	}
	public void setSingleTicketMaxLine(Long singleTicketMaxLine) {
		this.singleTicketMaxLine = singleTicketMaxLine;
	}
	public Long getSingleTicketMaxAmount() {
		return singleTicketMaxAmount;
	}
	public void setSingleTicketMaxAmount(Long singleTicketMaxAmount) {
		this.singleTicketMaxAmount = singleTicketMaxAmount;
	}
	public Short getAbandonRewardCollect() {
		return abandonRewardCollect;
	}
	public void setAbandonRewardCollect(Short abandonRewardCollect) {
		this.abandonRewardCollect = abandonRewardCollect;
	}
	
}
