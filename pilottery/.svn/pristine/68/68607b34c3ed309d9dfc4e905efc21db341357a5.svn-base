/*游戏控制*/
#left2{
	background:#3c4d55;
	width:204px;
	float:left;
	position:absolute; 
    top: 38px;
	bottom: 0;
	border-right:1px solid #d1d2d4;
	font-size:18px;
	color:#d2d4d4;
	padding-left:1px;
	}
#left2 ul{
	width:206px;
	height:300px;
	list-style: none;
	margin: 0px;
	padding: 0px;
	border: none;
	
	}

#left2 ul li{
	padding:13px 12px 11px 10px;
	width:202px;
	margin: 0px 0px 0px 1px;
	padding: 0px;
	border-bottom:1px solid #515f65;
	
	}
#left2 ul li a{
	padding:13px 12px 11px 10px;
    color:#fff;
	text-decoration:none;
	display: block;
	width:181px;
	
}

#left2 ul li a:hover{
	color:#000;
	padding:13px 12px 11px 10px;
	background-color:#27a0d7;
	text-decoration:none;
	text-indent:4px;

}
#left2 li.demo {
	background:url(../img/left-jtbj.png) no-repeat left top;
	padding:13px 12px 11px 10px;
	width:214px;
	color:#000;
	border:none;
	
}
#left2 li. a:demo{
	background:url(../img/left-jtbj.png) no-repeat left top;
	width:214px;
	color:#000;
	border:none;
	
}

#left2 li. a:hover demo {
    color:#000;
	text-indent:4px;
}


