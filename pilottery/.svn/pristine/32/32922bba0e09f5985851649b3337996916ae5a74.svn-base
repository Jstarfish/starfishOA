log4j.logger.notifyLog=debug,stdout,notify

log4j.appender.notify=org.apache.log4j.DailyRollingFileAppender 
log4j.appender.notify.DatePattern='.'yyyy-MM-dd  
log4j.appender.notify.File=/app/cncp/logs/notify.log   
log4j.appender.notify.layout=org.apache.log4j.PatternLayout   
log4j.appender.notify.layout.ConversionPattern=[%-5p]%d{yyy-MM-dd HH:mm:ss } - %m%n  
log4j.appender.notify.Append=true
   
log4j.appender.stdout=org.apache.log4j.ConsoleAppender   
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout   
log4j.appender.stdout.layout.ConversionPattern=[%d{yy/MM/dd HH:mm:ss}][%C-%M] %m%n 
    
log4j.appender.cncp=org.apache.log4j.DailyRollingFileAppender 
log4j.appender.cncp.DatePattern='.'yyyy-MM-dd  
log4j.appender.cncp.File=/app/cncp/logs/cncp_server.log   
log4j.appender.cncp.layout=org.apache.log4j.PatternLayout   
log4j.appender.cncp.layout.ConversionPattern=[%-5p]%d{yyy-MM-dd HH:mm:ss } - %m%n  
log4j.appender.cncp.Append=true
log4j.logger.cls.taishan=debug,stdout,cncp
log4j.logger.java.sql=debug,cncp

log4j.logger.io.vertx.ext.web.level=info,stdout,cncp
log4j.logger.io.vertx.level=info,stdout,cncp
log4j.logger.com.hazelcast.level=info,stdout,cncp
log4j.logger.io.netty.util.internal.PlatformDependent.level=SEVERE
log4j.logger.com.mchange=error,stdout,cncp


