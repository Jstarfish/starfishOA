CREATE OR REPLACE PACKAGE earea_type IS
   /****** 区域类型(0=全国/1=省/2=市/3=区县) ******/
   country                  /* 0=全国 */                CONSTANT NUMBER := 0;
   province                 /* 1=省 */                  CONSTANT NUMBER := 1;
   city                     /* 2=市 */                  CONSTANT NUMBER := 2;
   district                 /* 3=区县 */                CONSTANT NUMBER := 3;
END;
/