CREATE OR REPLACE PACKAGE eacc_status IS
   /****** 账户状态（1-可用，2-停用，3-异常） ******/
   available                /* 1-可用 */                  CONSTANT NUMBER := 1;
   stoped                   /* 2-停用 */                  CONSTANT NUMBER := 2;
   abnormal                 /* 3-异常 */                  CONSTANT NUMBER := 3;
END;
/
