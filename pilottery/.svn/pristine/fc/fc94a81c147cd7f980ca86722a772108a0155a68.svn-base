package cls.pilottery.oms.common.msg;

import java.io.Serializable;

/*
 * Message body used by UpdateGameAuth5011Req
 */
public class AreaGameCtrl implements Serializable {

	private static final long serialVersionUID = 2119275453540170574L;
	private Integer gameCode;
	private Integer status;
	private Integer sellStatus;
	private Integer payStatus;
	private Integer cancelStatus;
	
	public Integer getGameCode() {
		return gameCode;
	}
	public void setGameCode(Integer gameCode) {
		this.gameCode = gameCode;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public Integer getSellStatus() {
		return sellStatus;
	}
	public void setSellStatus(Integer sellStatus) {
		this.sellStatus = sellStatus;
	}
	public Integer getPayStatus() {
		return payStatus;
	}
	public void setPayStatus(Integer payStatus) {
		this.payStatus = payStatus;
	}
	public Integer getCancelStatus() {
		return cancelStatus;
	}
	public void setCancelStatus(Integer cancelStatus) {
		this.cancelStatus = cancelStatus;
	}
}
