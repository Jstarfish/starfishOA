package cls.pilottery.web.system.form;

import java.util.List;

import cls.pilottery.common.model.BaseEntity;
import cls.pilottery.web.system.model.User;

/**
 * 用户信息Form类
 */
public class UserForm extends BaseEntity {
	private static final long serialVersionUID = 3575568940677627795L;

	private User user;
	private List<Long> roleIds;

	private String loginId;
	private Integer status;
	private String institutionName;
	private String institutionCode;
	private String birthday;
	private Integer gander;
	private Integer isCollector;
	private String ex_password;
	private boolean isCollectorB;

	private Short oper;// 操作类型（1=新增；2更改）

	public Short getOper() {
		return oper;
	}

	public void setOper(Short oper) {
		this.oper = oper;
	}

	
	
	public Integer getIsCollector() {
		return isCollector;
	}

	public void setIsCollector(Integer isCollector) {
		this.isCollector = isCollector;
	}

	public String getEx_password() {
		return ex_password;
	}

	public void setEx_password(String ex_password) {
		this.ex_password = ex_password;
	}

	public Integer getGander() {
		return gander;
	}

	public void setGander(Integer gander) {
		this.gander = gander;
	}

	public String getBirthday() {
		return birthday;
	}

	public void setBirthday(String birthday) {
		this.birthday = birthday;
	}

	public String getInstitutionName() {
		return institutionName;
	}

	public void setInstitutionName(String institutionName) {
		this.institutionName = institutionName;
	}

	public List<Long> getRoleIds() {
		return roleIds;
	}

	public void setRoleIds(List<Long> roleIds) {
		this.roleIds = roleIds;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public String getLoginId() {
		return loginId;
	}

	public void setLoginId(String loginId) {
		this.loginId = loginId;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getInstitutionCode() {
		return institutionCode;
	}

	public void setInstitutionCode(String institutionCode) {
		this.institutionCode = institutionCode;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	public boolean isCollectorB() {
		return isCollectorB;
	}

	public void setCollectorB(boolean isCollectorB) {
		this.isCollectorB = isCollectorB;
	}

}
