{% extends "base.html" %}

{% block content %}
        {% if ffr=='0' %}
            <div>
			    <span id="page-topic">&nbsp;&nbsp;<input class="button" type="submit" value="Go Back" onclick="javascript:history.back();" />
			</div>
			<br /><br />
        {% else %}
            <!-- Page Head -->
			<div>
			    <span id="page-topic">&nbsp;&nbsp;Query Sale Ticket</span>
			</div>
			<br /><br />
			<div class="clear"></div> <!-- End .clear -->

			<div class="content-box"><!-- Search Content Box -->
                
                <div class="content-box-header">
                    
                    <h3>Search box</h3>
                    
                </div> <!-- End .content-box-header -->

                <div class="content-box-content">

                    <form action="/q_saleticket/" method="post">

                        <table width="100%" border="0" cellspacing="0" cellpadding="0" align="center">
                            <tbody>
                                <tr class="empty_white">
                                    <td width="100">Ticket TSN</td>
                                    <td width="250"><input class="text-input tsn-input" type="text" name="ticket_tsn" value="{{ticket_tsn}}" /></td>
                                    <td class="search_td"><input class="button" type="submit" value="Submit" /></td>
                                    <td class="search_td"></td>
                                </tr>
                            </tbody>
                        </table>
                    </form>
                </div> <!-- End .content-box-content -->
            </div> <!-- End .content-box -->
        {% end %}
            {% autoescape None %}
            <div class="content-box"><!-- Start Content Box -->
				
				<div class="content-box-header">
					
					<h3 class="gt">Sale Ticket TSN&nbsp;&nbsp;&nbsp;&nbsp;[&nbsp;&nbsp;&nbsp;&nbsp;{{ticket_tsn}}&nbsp;&nbsp;&nbsp;&nbsp;]&nbsp;&nbsp;{{error_info}}</h3>			
					<div class="clear"></div>
					
				</div> <!-- End .content-box-header -->
				
				<div class="content-box-content">
					<div class="tab-content default-tab"> <!-- This is the target div. id must match the href of this div's tab -->
                        {% if len(t_d)>0 %}
                        <table>
							<tr>
								<td width="100">reqfn_ticket</td><td class="gt">{{t_d['reqfn_ticket']}}</td>
                                <td width="100">rspfn_ticket</td><td class="gt">{{t_d['rspfn_ticket']}}&nbsp;&nbsp;&nbsp;&nbsp;{{t_d['unique_tsn']}}</td>
                            </tr>
                            <tr>
								<td width="100">Game</td><td class="gt">{{t_d['game_code']}}</td>
                                <td width="100">Issue Number</td><td class="gt">{{t_d['issue_number']}}</td>
                            </tr>
                            <tr>
								<td>from_sale</td><td class="gt">{{t_d['from_sale']}}</td>
                                <td>time_stamp</td><td class="gt">{{fmts(t_d['time_stamp'])}}</td>
                            </tr>
                            <tr>
								<td>issue_count</td><td class="gt">{{t_d['issue_count']}}</td>
                                <td>sale_start_issue</td><td class="gt">{{t_d['sale_start_issue']}}</td>
                            </tr>
                            <tr>
								<td>sale_start_issue_serial</td><td class="gt">{{t_d['sale_start_issue_serial']}}</td>
                                <td>sale_end_issue</td><td class="gt">{{t_d['sale_end_issue']}}</td>
                            </tr>
                            <tr>
								<td>total_bets</td><td class="gt">{{t_d['total_bets']}}</td>
                                <td>ticket_amount</td><td class="gt">{{t_d['ticket_amount']}}</td>
                            </tr>
                            <tr>
								<td>commission_amount</td><td class="gt">{{t_d['commission_amount']}}</td>
                                <td>claiming_scope</td><td class="gt">{{t_d['claiming_scope']}}</td>
                            </tr>
                            <tr>
								<td>draw_time_stamp</td><td class="gt">{{fmts(t_d['draw_time_stamp'])}}</td>
                                <td>is_train</td><td class="gt">{{t_d['is_train']}}</td>
                            </tr>
                            <tr>
								<td>betline_count</td><td colspan="3" class="gt">{{t_d['betline_count']}}</td>
                            </tr>
                            <tr>
								<td>bet_string</td><td colspan="3" class="gt">{{t_d['bet_string']}}</td>
                            </tr>
                            <tr>
								<td>ticket</td><td colspan="3" class="gt">{{dump(t_d['ticket'])}}</td>
                            </tr>
                            <tr>
								<td>area_code</td><td class="gt">{{t_d['area_code']}}</td>
                                <td>area_type</td><td class="gt">{{t_d['area_type']}}</td>
                            </tr>
                            <tr>
								<td>agency_code</td><td class="gt">{{t_d['agency_code']}}</td>
                                <td>terminal_code</td><td class="gt">{{t_d['terminal_code']}}</td>
                            </tr>
                            <tr>
								<td>teller_code</td><td class="gt">{{t_d['teller_code']}}</td>
                                <td>ap_code</td><td class="gt">{{t_d['ap_code']}}</td>
                            </tr>
                            <tr>
								<td>is_cancel</td><td class="gt">{{t_d['is_cancel']}}</td>
                                <td>from_cancel</td><td class="gt">{{t_d['from_cancel']}}</td>
                            </tr>
                            <tr>
								<td>reqfn_ticket_cancel</td><td class="gt">{{t_d['reqfn_ticket_cancel']}}</td>
                                <td>rspfn_ticket_cancel</td><td class="gt">{{t_d['rspfn_ticket_cancel']}}</td>
                            </tr>
                            <tr>
								<td>time_stamp_cancel</td><td class="gt">{{t_d['time_stamp_cancel']}}</td>
                                <td>agency_code_cancel</td><td class="gt">{{t_d['agency_code_cancel']}}</td>
                            </tr>
                            <tr>
								<td>terminal_code_cancel</td><td class="gt">{{t_d['terminal_code_cancel']}}</td>
                                <td>teller_code_cancel</td><td class="gt">{{t_d['teller_code_cancel']}}</td>
                            </tr>
                            <tr>
								<td>ap_code_cancel</td><td colspan="3" class="gt">{{t_d['ap_code_cancel']}}</td>
                            </tr>
                            <tr>
								<td>vfyc</td><td colspan="3" class="gt">{{dump(t_d['vfyc'])}}</td>
                            </tr>
						</table>
                        {% else %}
                        <br>
                        {% end %}
					</div> <!-- End #tab1 -->
				</div>
			</div> <!-- End .content-box -->

{% end %}
