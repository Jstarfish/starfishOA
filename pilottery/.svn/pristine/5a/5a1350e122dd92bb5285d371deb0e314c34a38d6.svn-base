CREATE OR REPLACE PACKAGE edevice_status IS
   /****** 设备状态(1=启用/2=暂停/3=停用) ******/
   enabled                  /* 1=启用 */                CONSTANT NUMBER := 1;
   disabled                 /* 2=暂停 */                CONSTANT NUMBER := 2;
   cancelled                /* 3=停用 */                CONSTANT NUMBER := 3;
END;
/