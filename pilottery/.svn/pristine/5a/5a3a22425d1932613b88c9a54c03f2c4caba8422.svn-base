<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    android:background="#ffffff"
    android:orientation="vertical" >
    <!--充值-->   
    <LinearLayout
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="horizontal" 
        >
		<TextView
	 	    android:id="@+id/num"
	 	    android:layout_width="wrap_content"
	    	android:layout_height="wrap_content" 
	    	android:layout_marginTop="20dp"
	    	android:textSize="@dimen/BodyFont"
	   		android:textColor="#000000"
	   		android:text="@string/withdraw_station_code"
 	    />   
        <TextView
            android:id="@+id/code"
            android:layout_width="wrap_content"
    		android:layout_height="wrap_content" 
    		android:layout_marginTop="20dp"
    		android:layout_marginLeft="20dp"
    		android:textSize="@dimen/BodyFont"
    		android:gravity="right"
    		android:textColor="#000000"
            />
    </LinearLayout>

	<TextView
 	    android:id="@+id/credit"
 	    android:layout_width="wrap_content"
    	android:layout_height="wrap_content" 
    	android:layout_marginTop="30dp"
    	android:textSize="@dimen/BodyFont"
   		android:textColor="#000000"
   		android:text="@string/rc_input_money" 
 	    /> 
  	<EditText
 	    android:id="@+id/money"
 	    android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:inputType="number"
    	android:hint="@string/rc_input_money1"
   		android:textColor="#000000"
 	    /> 
 	 <TextView
 	    android:layout_width="wrap_content"
    	android:layout_height="wrap_content" 
    	android:layout_marginTop="30dp"
    	android:textSize="@dimen/BodyFont"
   		android:textColor="#000000"
   		android:text="@string/rc_input_keywords" 
 	    /> 

 	 <EditText
 	     android:id="@+id/key_word"
 	     android:layout_width="match_parent"
 	     android:layout_height="wrap_content"
 	     android:hint="@string/rc_input_keywords1"
 	     android:inputType="numberPassword"
 	     
 	     android:textColor="#000000" />

 	 <Button
 	    android:id="@+id/ensure"
        android:layout_width="match_parent"
    	android:layout_height="wrap_content" 
    	android:layout_marginTop="30dp"
    	style="@style/common_btn"
    	android:text="@string/string_comfirm"
 	     />
</LinearLayout>
