create or replace function f_cncp_check_dealer_status(p_dealer char)
  return boolean
  result_cache
  relies_on(cncp_inf_dealers)
is
/********************************************************************************/
  ------------------- 适用于检查渠道商是否为正常状态 ----------------------------
  ---- add by 陈震: 2016-09-09
/********************************************************************************/
  v_count number(1);
begin
  select count(*) 
    into v_count 
    from cncp_inf_dealers
   where dealer_code = p_dealer
     and dealer_status = ecncp_dealer_status.enabled;
  if v_count = 1 then
    return true;
  else
    return false;
  end if;
end;
