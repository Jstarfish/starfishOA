<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cls.pilottery.web.logistics.dao.LogisticsDao">
    <select id="getWarehousename" parameterType="string" resultType="string">
        SELECT A.WAREHOUSE_NAME FROM WH_INFO A WHERE A.WAREHOUSE_CODE
        =#{warehouseCode}
    </select>
    <select id="getUserName" parameterType="string" resultType="string">
        SELECT
        A.ADMIN_REALNAME FROM ADM_INFO A WHERE A.ADMIN_ID =#{userCode}
    </select>
    <select id="getPayout" resultType="cls.pilottery.web.logistics.form.PayoutForm"
        parameterType="string"
    >
        select b.AGENCY_NAME outlet,a.PAY_TIME payoutDate from FLOW_PAY a left join
        INF_AGENCYS b on a.PAY_AGENCY=b.AGENCY_CODE where 1=1 and 
        a.IDENTITY_CODE=#{_parameter}
    </select>
</mapper>