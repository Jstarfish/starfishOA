<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:ignore="Overdraw" >

<!-- 新增出货单--> 
	
	
	<LinearLayout
        android:layout_width="match_parent"
   		android:layout_height="wrap_content"
   		android:paddingLeft="45dp"
   		android:paddingRight="10dp"
	    android:paddingTop="5dp"
    	android:paddingBottom="5dp"
    	android:background="@drawable/ListTitleBackGround"
        >
        <TextView	            
            android:layout_width="0dp"
   			android:layout_height="wrap_content" 
   			android:layout_weight="5"
			android:gravity="center"
    		android:textSize="@dimen/BodyFont"
    		android:textColor="@drawable/black"
    		android:text="@string/dfg_num" 
            />
        <TextView	            
            android:layout_width="0dp"
   			android:layout_height="wrap_content" 
    		android:textSize="@dimen/BodyFont"
    		android:layout_weight="3"
			android:gravity="center"
    		android:textColor="@drawable/black"
    		android:text="@string/order_lotery_name" 
            />
        <TextView
            android:id="@+id/tv"	            
            android:layout_width="0dp"
   			android:layout_height="wrap_content" 
    		android:textSize="@dimen/BodyFont"
    		android:layout_weight="3"
			android:gravity="right"
    		android:textColor="@drawable/black"
    		android:text="@string/order_lotery_count" 
            />
    </LinearLayout>
    <View android:layout_width="match_parent" android:layout_height="2dp" android:background="@drawable/black" />
    <ListView
        android:id="@+id/list1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fadeScrollbars="false"
        android:layout_weight="3"
         >
	</ListView>
	<Button
	   	android:id="@+id/ensure"
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:layout_marginLeft="15dp"
    	android:layout_marginRight="15dp"
	    android:layout_marginTop="15dp"
	    android:layout_marginBottom="15dp"
    	style="@style/common_btn"
    	android:text="@string/string_comfirm"
	    />
