{% extends "base.html" %}

{% block content %}
{% autoescape None %}
			<!-- Page Head -->
			<div>
			    <span id="page-topic">&nbsp;&nbsp;Win Ticket</span>
			</div>
			
			<br /><br />
			
			<div class="clear"></div> <!-- End .clear -->
			
			<div class="content-box"><!-- Search Content Box -->
                
                <div class="content-box-header">
                    
                    <h3>Search box</h3>
                    
                </div> <!-- End .content-box-header -->

                <div class="content-box-content">

                    <form action="/winticket/" method="post">

                        <table width="100%" border="0" cellspacing="0" cellpadding="0" align="center">
                            <tbody>
                                <tr class="empty_white">
                                    <td width="100">Select Game</td>
                                    <td width="100">
                                        <select name="game_code" class="game-input">
                                            {% for game in game_list %}
                                            <option value="{{game}}" {% if game_code==game %}Selected{% end %}>{{game}}</option>
                                            {% end %}
                                        </select>
                                    </td>
                                    <td width="100">Issue Number</td>
                                    <td width="120"><input class="text-input issue-input" type="text" name="issue_number" value="{{issue_number}}" /></td>
                                    <td width="100">Agency Code</td>
                                    <td width="120"><input class="text-input issue-input" type="text" name="agency_code" value="{{agency_code}}" /></td>
                                    <td class="search_td"><input class="button" type="submit" value="Submit" /></td>
                                    <td class="search_td"></td>
                                </tr>
                            </tbody>
                        </table>

                    </form>
                    
                </div> <!-- End .content-box-content -->
                
            </div> <!-- End .content-box -->
            
            <div class="content-box"><!-- Start Content Box -->
				
				<div class="content-box-header">
					
					<h3>Ticket List</h3>
					<div class="clear"></div>
					
				</div> <!-- End .content-box-header -->
				
				<div class="content-box-content">
					<div class="tab-content default-tab"> <!-- This is the target div. id must match the href of this div's tab -->
						<table>
							
							<thead>
								<tr>
								   <th>TSN</th>
                                   <th>Issue Count</th>
                                   <th>Is Paid</th>
                                   <th>Pay Time</th>
                                   <th>Big Win</th>
								   <th>Win Amount</th>
                                   <th>Hd Amt</th>
                                   <th>Hd Cnt</th>
                                   <th>Ld Amt</th>
                                   <th>Ld Cnt</th>
								   <th>prize_count</th>
								</tr>
							</thead>

                            <tbody>
							{% if len(t_l)>0 %}
                                {% for row in t_l %}
                                <tr>
                                    <td class="gt">{{row['rspfn_ticket']}}&nbsp;&nbsp;<a href="/q_winticket/?ffr=0&ticket_tsn={{row['rspfn_ticket']}}&game_code={{game_code}}&issue_number={{issue_number}}" title="View Ticket Detail"><img src="/static/image/detail.png" /></a></td>
                                    <td>{{row['issue_count']}}</td>
                                    <td>{{row['paid_status']}}</td>
                                    <td>{{fmts(row['time_stamp_pay'])}}</td>
                                    <td>{{row['is_big_winning']}}</td>
                                    <td>{{row['winning_amount_tax']}}</td>
                                    <td>{{row['hd_winning']}}</td>
                                    <td>{{row['hd_count']}}</td>
                                    <td>{{row['ld_winning']}}</td>
                                    <td>{{row['ld_count']}}</td>
                                    <td>{{row['prize_count']}}</td>
                                </tr>
                                {% end %}
                            {% else %}
                                <br>
                            {% end %}
                            </tbody>
							
							<tfoot>
								<tr>
									<td colspan="8">
										<div class="pagination">{{ page_str }}</div> <!-- End .pagination -->
									</td>
								</tr>
							</tfoot>
						</table>
					</div> <!-- End #tab1 -->
				</div>
			</div> <!-- End .content-box -->

{% end %}
