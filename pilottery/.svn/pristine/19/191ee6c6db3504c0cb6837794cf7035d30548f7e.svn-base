package cls.taishan.web.service.Impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import cls.taishan.web.dao.Impl.WingCommonDaoImpl;
import cls.taishan.web.model.WingEUserLoginInfo;
import cls.taishan.web.model.WingWithdrawInputParam;
import cls.taishan.web.model.WingWithdrawOutputParam;
import cls.taishan.web.service.WingWithdrawService;
import lombok.extern.log4j.Log4j;

@Log4j
@Service
public class WingWithdrawServiceImpl implements WingWithdrawService {
	
	@Autowired
	public WingCommonDaoImpl commDao;
	
	@Override
	public int doWithdraw(WingWithdrawInputParam in, WingWithdrawOutputParam out) {
		WingEUserLoginInfo userInfo = new WingEUserLoginInfo();
		
		int r = commDao.getEUserLoginInfo(in.getAccount(), userInfo);
		if (r != 0){
			out.setErrorCode(r);
			out.setErrorMessage(commDao.errMsg);
			return r;
		}
		
		
		
		return 0;
	}

}
