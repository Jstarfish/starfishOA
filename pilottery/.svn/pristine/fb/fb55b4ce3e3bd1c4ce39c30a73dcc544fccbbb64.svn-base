package cls.taishan.web.dao;

public class wingChargeInputDao {
	private String eUser;
	private String account;
	private long amount;
	private String userAcc;
	private String otp;
	private String reqFlow;
	
	// 检查所有参数是否为空(充值交易)
	public boolean checkNull(){
		if (eUser == null || eUser.isEmpty()) return true;
		if (account == null || account.isEmpty()) return true;
		if (userAcc == null || userAcc.isEmpty()) return true;
		if (otp == null || otp.isEmpty()) return true;
		if (reqFlow == null || reqFlow.isEmpty()) return true;
		if (amount <= 0) return true;

		return false;
	}

	public String geteUser() {
		return eUser;
	}

	public void seteUser(String eUser) {
		this.eUser = eUser;
	}

	public String getAccount() {
		return account;
	}

	public void setAccount(String account) {
		this.account = account;
	}

	public long getAmount() {
		return amount;
	}

	public void setAmount(long amount) {
		this.amount = amount;
	}

	public String getUserAcc() {
		return userAcc;
	}

	public void setUserAcc(String userAcc) {
		this.userAcc = userAcc;
	}

	public String getOtp() {
		return otp;
	}

	public void setOtp(String otp) {
		this.otp = otp;
	}

	public String getReqFlow() {
		return reqFlow;
	}

	public void setReqFlow(String reqFlow) {
		this.reqFlow = reqFlow;
	}

	@Override
	public String toString() {
		return "wingChargeInput [eUser=" + eUser + ", account=" + account + ", amount=" + amount + ", userAcc="
				+ userAcc + ", otp=" + otp + ", reqFlow=" + reqFlow + "]";
	}

}
